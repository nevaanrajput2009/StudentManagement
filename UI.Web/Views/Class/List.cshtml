@model UI.Web.Models.ClassModel
<h1>Class List</h1>

<a href="#" class="btn btn-primary">Create New</a>
<br />
<table class="table table-bordered">
    <thead>
        <tr>
            <th>
                Class Id
            </th>
            <th>
                Class Name
            </th>
            <th>
                Is Active
            </th>
            <th>
                Action
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var row in @Model.classList)
        {
            <tr>
                <td>
                    @row.ClassId
                </td>
                <td>
                    @row.ClassName
                </td>
                <td>
                    @row.IsActive
                </td>
                <td>
                    <a href="" class="btn btn-primary">Edit</a>
                    <a href="" class="btn btn-danger">Delete</a>
                </td>

            </tr>
        }
    </tbody>
</table>